# Environment Configuration for FarmSense React Frontend

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:8000
REACT_APP_API_VERSION=v1

# Application Environment
REACT_APP_ENVIRONMENT=development
NODE_ENV=development

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_PWA=false
REACT_APP_ENABLE_SERVICE_WORKER=false

# Internationalization
REACT_APP_DEFAULT_LANGUAGE=en
REACT_APP_SUPPORTED_LANGUAGES=en,hi,mr,gu,bn,ta,te,kn

# Authentication & Security
REACT_APP_JWT_SECRET=your-jwt-secret-key
REACT_APP_TOKEN_EXPIRY=3600
REACT_APP_REFRESH_TOKEN_EXPIRY=604800

# Third-party Services
REACT_APP_AI_SERVICE_URL=https://api.gemini.ai
REACT_APP_AI_API_KEY=your-gemini-api-key

# Analytics & Monitoring (Optional)
REACT_APP_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
REACT_APP_SENTRY_DSN=https://your-sentry-dsn
REACT_APP_MIXPANEL_TOKEN=your-mixpanel-token

# PWA Configuration
REACT_APP_PWA_NAME=FarmSense
REACT_APP_PWA_SHORT_NAME=FarmSense
REACT_APP_PWA_THEME_COLOR=#4CAF50
REACT_APP_PWA_BACKGROUND_COLOR=#ffffff

# Mobile App Configuration (Capacitor)
REACT_APP_CAPACITOR_APP_ID=com.farmsense.app
REACT_APP_CAPACITOR_APP_NAME=FarmSense

# Development Tools
REACT_APP_ENABLE_DEBUG=true
REACT_APP_LOG_LEVEL=debug

# Production Override Example:
# For production deployment, copy this to .env.production and update values:
# REACT_APP_API_BASE_URL=https://api.farmsense.in
# REACT_APP_ENVIRONMENT=production
# REACT_APP_ENABLE_ANALYTICS=true
# REACT_APP_ENABLE_PWA=true
# REACT_APP_ENABLE_SERVICE_WORKER=true
# REACT_APP_ENABLE_DEBUG=false
# REACT_APP_LOG_LEVEL=error

# Staging Environment Example:
# For staging deployment, copy this to .env.staging and update values:
# REACT_APP_API_BASE_URL=https://staging-api.farmsense.in
# REACT_APP_ENVIRONMENT=staging
# REACT_APP_ENABLE_ANALYTICS=false
# REACT_APP_ENABLE_PWA=true
# REACT_APP_ENABLE_DEBUG=true
# REACT_APP_LOG_LEVEL=info